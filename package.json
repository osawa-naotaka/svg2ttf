{
  "name": "@lulliecat/svg2ttf",
  "version": "6.0.6",
  "type": "module",
  "description": "Converts SVG font to TTF font",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "font",
    "ttf",
    "svg",
    "convertor"
  ],
  "author": "osawa naotaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osawa-naotaka/svg2ttf.git"
  },
  "bin": {
    "svg2ttf": "./build/svg2ttf.js"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "lint": "biome check ./src",
    "format": "biome format --write ./src",
    "test": "bun run lint && bun run build && vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build": "tsup --config tsup.config.ts",
    "update_fixture": "./build/svg2ttf.js --ts 1457357570703 test/fixtures/test.svg test/fixtures/test.ttf"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11",
    "argparse": "^2.0.1",
    "cubic2quad": "^1.2.1",
    "svgpath": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/argparse": "^2.0.17",
    "@types/node": "^24.9.1",
    "@vitest/ui": "^3.2.4",
    "opentype.js": "^1.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
